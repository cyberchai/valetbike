<h2>Add Bikes</h2>
<%= form_with(model: @bike, url: bikes_path) do |f| %>


<div class="form-group">
    <%= f.label :Select_Stations %>
    <%= f.collection_select :current_station_id, Station.all, :id, :name %>
</div>
 
<div class="form-group">
  <%= f.label :identifier %>
  <%= f.number_field :identifier, class: "form-control" %>
</div>


<div class="form-group">
  <%= f.submit %>
</div>
 
<% end %>